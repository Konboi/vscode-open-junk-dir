{
  "name": "open-junk-dir",
  "displayName": "open junk dir",
  "description": "create a junk directory",
  "version": "0.0.2",
  "publisher": "Konboi",
  "engines": {
    "vscode": "^1.30.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Konboi/vscode-open-junk-dir"
  },
  "homepage": "https://github.com/Konboi/vscode-open-junk-dir",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openJunkDir"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "openJunkDir",
      "properties": {
        "openJunkDir.junkFileDir": {
          "type": "string",
          "default": "~/junk",
          "description": "root directory to make junk file"
        },
        "openJunkDir.junkFileFormat": {
          "type": "string",
          "default": "/YYYY/MM/DD/YYYYMMDD_hmmss",
          "description": "junk file directory format (DAY.js format)"
        }
      }
    },
    "commands": [
      {
        "command": "extension.openJunkDir",
        "title": "Open Junk Dir"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "vscode": "1.1.37",
    "tslint": "6.1.3",
    "@types/mocha": "9.0.0",
    "@types/node": "16.10.1",
    "typescript": "4.4.2"
  },
  "dependencies": {
    "@types/mkdirp": "^1.0.2",
    "@types/untildify": "^4.0.0",
    "dayjs": "^1.10.6",
    "mkdirp": "^1.0.4",
    "untildify": "^4.0.0"
  }
}
